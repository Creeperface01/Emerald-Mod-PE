<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.razvanmcrafter.addon.emeraldmod"
	android:versionCode="13"
	android:versionName="1.5.1">

	<uses-feature
		android:name="android.hardware.touchscreen"
		android:required="false" />

	<uses-permission android:name="net.zhuoweizhang.mcpelauncher.ADDON" />
	
	<uses-sdk android:minSdkVersion="15" android:targetSdkVersion="25" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
		<meta-data
			android:name="net.zhuoweizhang.mcpelauncher.api.nativelibname"
        	android:value="theluckycoder.emeraldmod" />
    	<meta-data
			android:name="net.zhuoweizhang.mcpelauncher.api.targetmcpeversion"
			android:value="1.0.0.16"/>
		<activity
            android:name="net.theluckycoder.emeraldmod.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
					
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>

